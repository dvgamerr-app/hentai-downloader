{
  "name": "hentai-downloader",
  "title": "Hentai Downloader",
  "version": "3.0.0",
  "description": "Manga Hentai in e-hentai.org and exhentai.org downloader.",
  "main": "./src/main.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "echo \"No linting configured\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/touno-io/hentai-downloader.git"
  },
  "keywords": [
    "exhentai",
    "e-hentai",
    "hentai",
    "downloader",
    "manga",
    "dark",
    "doujin"
  ],
  "bugs": {
    "url": "https://github.com/touno-io/hentai-downloader/issues"
  },
  "homepage": "https://github.com/touno-io/hentai-downloader#readme",
  "author": {
    "name": "Kananek Thongkam",
    "email": "info.dvgamer@gmail.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./src/icons/icon.ico"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "hentai_downloader"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        },
        {
          "name": "@electron-forge/publisher-github",
          "config": {
            "repository": {
              "owner": "me",
              "name": "awesome-thing"
            },
            "prerelease": true
          }
        }
      ]
    }
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "vue": "^2.7.10"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.65",
    "@electron-forge/maker-deb": "^6.0.0-beta.65",
    "@electron-forge/maker-rpm": "^6.0.0-beta.65",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.65",
    "@electron-forge/maker-zip": "^6.0.0-beta.65",
    "@electron-forge/plugin-webpack": "^6.0.0-beta.65",
    "@touno-io/debuger": "^2.4.1",
    "electron": "20.1.0",
    "electron-settings": "^4.0.2",
    "eslint": "^8.23.0",
    "eslint-plugin-vue": "^9.4.0",
    "vue-loader": "^17.0.0",
    "vue-template-compiler": "^2.7.10"
  }
}
